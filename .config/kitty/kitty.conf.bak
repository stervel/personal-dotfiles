# vim:ft=kitty
# wayland_titlebar_color background

# Font Config
# font_family		 Iosevka SS14 Expanded
# bold_font		 Iosevka SS14 Bold Expanded
# italic_font	     Iosevka SS14 Italic Expanded
# bold_italic_font Iosevka SS14 Bold Italic Expanded
font_size		15.0

# Seti-UI + Custom
symbol_map U+E5FA-U+E62B Symbols Nerd Font
# Devicons
symbol_map U+E700-U+E7C5 Symbols Nerd Font
# Font Awesomm
symbol_map U+F000-U+F2E0 Symbols Nerd Font
# Font Awesome Extensio
symbol_map U+E200-U+E2A9 Symbols Nerd Font
# Material Design Icon
symbol_map U+F500-U+FD46 Symbols Nerd Font
# Weather
symbol_map U+E300-U+E3EB Symbols Nerd Font
# Octicons
symbol_map U+F400-U+F4A8,U+2665,U+26A1,U+F27C Symbols Nerd Font
# Powerline Extra Symbol
symbol_map U+E0A3,U+E0B4-U+E0C8,U+E0CC-U+E0D2,U+E0D4 Symbols Nerd Font
# IEC Power Symbols
symbol_map U+23FB-U+23FE,U+2b58 Symbols Nerd Font
# Font Logos
symbol_map U+F300-U+F313 Symbols Nerd Font
# Pomicon
symbol_map U+E000-U+E00D Symbols Nerd Font

# BEGIN_KITTY_THEME
# Material
include current-theme.conf
# END_KITTY_THEME


# BEGIN_KITTY_FONTS
font_family      family='Iosevka Term SS14' style=Extended
bold_font        family='Iosevka Term SS14' style='Bold Extended'
italic_font      family='Iosevka Term SS14' style='Extended Italic'
bold_italic_font family='Iosevka Term SS14' style='Bold Extended Italic'
# END_KITTY_FONTS